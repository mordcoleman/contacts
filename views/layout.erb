<!doctype html>
  <html lang="en-US">
  <link rel="stylesheet" href="/stylesheets/contact_application.css">

  <% if session[:error]%>
  <div class="flash error">
    <p><%= session.delete(:error) %></p>
  </div>
  <% end %>

  <% if session[:success] %>
  <div class="flash success">
    <p><%= session.delete(:success) %></p>
  </div>
  <% end %>

  <main>
      <%== yield %>
  </main>
</html>
